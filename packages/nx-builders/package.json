{
  "name": "@digiportal/nx-builders",
  "version": "5.2.4",
  "description": "TSC compiler for NX.",
  "repository": "https://gitlab.tailored-apps.com/ckilic/nx-test",
  "author": "Backend <backend@digiportal.com>",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "@digiportal:registry": "https://npm.pkg.github.com"
  },
  "schematics": "./schematics.json",
  "builders": "./builders.json",
  "scripts": {
    "build": "tsup-node",
    "postbuild": "cpy --cwd=src --parents --dot '**/*.json' ../dist/",
    "dev:start": "tsup-node --watch",
    "lint": "prettier --loglevel warn --write src/ && eslint --ext .ts,.js,.tsx,.jsx --fix src/",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx src/",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i",
    "docs:jsdoc": "typedoc src/index.ts --readme none"
  },
  "lint-staged": {
    "./**/*.{ts,js,tsx,jsx}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "./**/*.{json,md}": [
      "prettier --loglevel warn --write"
    ]
  },
  "peerDependencies": {
    "@nrwl/workspace": "*",
    "ts-node": ">= 8.10.0",
    "ts-node-dev": ">= 1.0.0",
    "tsc-watch": ">= 4.2.0",
    "tsconfig-replace-paths": "^0.0.11",
    "typescript": ">= 3.9.0"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1302.3",
    "@angular-devkit/core": "^13.2.3",
    "@angular-devkit/schematics": "^13.2.3",
    "@nrwl/cli": "^13.8.2",
    "@nrwl/devkit": "^13.8.2",
    "@digiportal/deep-merge": "*",
    "@digiportal/nx-tools": "*",
    "delay": "^5.0.0",
    "dotenv": "^16.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/tsc-watch": "^4.2.0"
  },
  "engines": {
    "node": ">= 16"
  }
}
