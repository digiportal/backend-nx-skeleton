{
  "$schema": "http://json-schema.org/schema",
  "$id": "@digiportal/nx-nest/generator",
  "title": "@digiportal/nx-nest/generator",
  "description": "Generate nestjs partials.",
  "examples": [],
  "type": "object",
  "properties": {
    "skipFormat": {
      "description": "Skip formatting after the initiation.",
      "type": "boolean",
      "aliases": ["skip-format"]
    },

    "silent": {
      "description": "Run in silent mode.",
      "type": "boolean"
    },

    "force": {
      "description": "Do not prompt for overwrites.",
      "type": "boolean"
    },

    "verbose": {
      "description": "Set this to debug mode.",
      "type": "boolean"
    }
  },
  "required": []
}
